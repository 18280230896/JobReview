<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.llg.mapper.TaskFileMapper">
	<resultMap type="TaskFile" id="taskFile">
		<id property="id" column="file_id"/>
		<result property="name" column="file_name"/>
		<result property="path" column="file_path"/>
		<association property="subject" javaType="Subject">
			<id property="id" column="subject_id"/>
		</association>
		<association property="student" javaType="Student">
			<id property="id" column="student_id"/>
		</association>
	</resultMap>
</mapper>