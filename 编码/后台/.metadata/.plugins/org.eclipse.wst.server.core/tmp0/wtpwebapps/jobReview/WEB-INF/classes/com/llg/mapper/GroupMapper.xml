<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.llg.mapper.GroupMapper">
	<resultMap type="Group" id="group">
		<id property="id" column="group_id"/>
		<result property="Num" column="group_num"/>
		<result property="name" column="group_name"/>
		<association property="leader" javaType="Student">
			<id property="id" column="leader_id"/>
		</association>
		<association property="c" javaType="Class">
			<id property="id" column="class_id"/>
		</association>
	</resultMap>
</mapper>