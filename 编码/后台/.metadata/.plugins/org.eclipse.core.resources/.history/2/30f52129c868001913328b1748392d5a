<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/path" xmlns:path="http://www.example.org/path" elementFormDefault="qualified">
	<xsd:element name="path" type="path:pathType"></xsd:element>
	<xsd:complexType name="pathType">
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">
					<xsd:sequence>
						<xsd:element name="localJobPath" type="xsd:string"></xsd:element>
						<xsd:element name="localTempPath" type="xsd:string"></xsd:element>
						<xsd:element name="virtualJobPath" type="xsd:string"></xsd:element>
					</xsd:sequence> 
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
</xsd:schema>