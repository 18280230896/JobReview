var javaEditor = CodeMirror.fromTextArea(document.getElementById("code"), {
            lineNumbers: true,
            matchBrackets: true,
            mode: "text/x-java"
        });
$("#run").click(function(){
	var code = javaEditor.getValue();
	console.log(code);
	$.ajax({
		url:"run.action",
		type:"post",
		dataType:"json",
		data:{"code":code},
		success:function(result){
			
		}
	});
});