	<!-- 本地路径，用于存放编译运行所产生的临时文档 -->
	<path:localTempPath>D:\jobReview\temp\</path:localTempPath>
	<!-- 虚拟路径，配置tomcat虚拟路径时将此路径与localJobPath关联，用于在网页上访问截图 -->
	<path:virtualJobPath>/file/job/</path:virtualJobPath>
</path:path>